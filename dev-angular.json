{
    "family": "dev-ccs-sso",
    "networkMode": "awsvpc",
    "taskRoleArn": "arn:aws:iam::111314917260:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::111314917260:role/ecsTaskExecutionRole",
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "sand-ccs-sso",
            "image": "111314917260.dkr.ecr.eu-west-2.amazonaws.com/demo-angular:latest",
            "cpu": 0,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dev-ccs-sso",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}

